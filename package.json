{
  "dependencies": {
    "sass": "^1.49.8",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.9.0",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "wipe-sass": "powershell Remove-Item stylesheets/* -Recurse",
    "dev": "npm-run-all --parallel watch-sass watch-ts start",
    "start": "http-server",
    "watch-sass": "sass --watch sass:stylesheets --no-source-map",
    "watch-ts": "tsc --watch"
  }
}
